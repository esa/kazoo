/* CIF PROCESS (250, 150), (150, 75) */
process Demo_Sdl_To_C;
    /* CIF START (320, 10), (70, 35) */
    START;
        /* CIF NEXTSTATE (320, 60), (70, 35) */
        NEXTSTATE Wait;
    /* CIF state (320, 60), (70, 35) */
    state Wait;
        /* CIF input (320, 115), (70, 35) */
        input Pulse;
            /* CIF PROCEDURECALL (272, 170), (164, 35) */
            call writeln('SDL Pulse in C');
            /* CIF NEXTSTATE (320, 225), (70, 35) */
            NEXTSTATE -;
    endstate;
    /* CIF state (450, 10), (70, 35) */
    state Wait;
    endstate;
endprocess Demo_Sdl_To_C;